<h1> Transaction for <%= user.username %> </h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>category</th>
            <th>sum</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {%>
            <tr>
                <td><%= item._id %></td>
               <td><%= item.total %></td>
            </tr>
            <% }) %>
    </tbody>
</table>
